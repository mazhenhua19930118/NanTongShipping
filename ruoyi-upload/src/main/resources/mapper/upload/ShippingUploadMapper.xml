<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.upload.mapper.ShippingUploadMapper">
    
    <resultMap type="ShippingUpload" id="ShippingUploadResult">
        <result property="id"    column="id"    />
        <result property="relatedId"    column="related_id"    />
        <result property="fileName"    column="file_name"    />
        <result property="originFileName"    column="origin_file_name"    />
        <result property="filePath"    column="file_path"    />
        <result property="fileType"    column="file_type"    />
        <result property="uploadType"    column="upload_type"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
    </resultMap>

    <sql id="selectShippingUploadVo">
        select id, related_id, file_name, origin_file_name, file_path, file_type, create_by, create_time,upload_type  from shipping_upload
    </sql>

    <select id="selectShippingUploadList" parameterType="ShippingUpload" resultMap="ShippingUploadResult">
        <include refid="selectShippingUploadVo"/>
        <where>  
            <if test="relatedId != null "> and related_id = #{relatedId}</if>
            <if test="fileName != null  and fileName != ''"> and file_name like concat('%', #{fileName}, '%')</if>
            <if test="originFileName != null  and originFileName != ''"> and origin_file_name like concat('%', #{originFileName}, '%')</if>
            <if test="filePath != null  and filePath != ''"> and file_path = #{filePath}</if>
            <if test="fileType != null  and fileType != ''"> and file_type = #{fileType}</if>
            <if test="uploadType != null  and uploadType != ''"> and upload_type = #{uploadType}</if>
        </where>
    </select>
    
    <select id="selectShippingUploadById" parameterType="Long" resultMap="ShippingUploadResult">
        <include refid="selectShippingUploadVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertShippingUpload" parameterType="ShippingUpload" useGeneratedKeys="true" keyProperty="id">
        insert into shipping_upload
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="relatedId != null">related_id,</if>
            <if test="fileName != null">file_name,</if>
            <if test="originFileName != null">origin_file_name,</if>
            <if test="filePath != null">file_path,</if>
            <if test="fileType != null">file_type,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="uploadType != null">upload_type,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="relatedId != null">#{relatedId},</if>
            <if test="fileName != null">#{fileName},</if>
            <if test="originFileName != null">#{originFileName},</if>
            <if test="filePath != null">#{filePath},</if>
            <if test="fileType != null">#{fileType},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="uploadType != null">#{uploadType},</if>
         </trim>
    </insert>

    <update id="updateShippingUpload" parameterType="ShippingUpload">
        update shipping_upload
        <trim prefix="SET" suffixOverrides=",">
            <if test="relatedId != null">related_id = #{relatedId},</if>
            <if test="fileName != null">file_name = #{fileName},</if>
            <if test="originFileName != null">origin_file_name = #{originFileName},</if>
            <if test="filePath != null">file_path = #{filePath},</if>
            <if test="fileType != null">file_type = #{fileType},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="uploadType != null">upload_type = #{uploadType},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteShippingUploadById" parameterType="Long">
        delete from shipping_upload where id = #{id}
    </delete>

    <delete id="deleteShippingUploadByIds" parameterType="String">
        delete from shipping_upload where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>